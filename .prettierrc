{
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "printWidth": 120,
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "importOrder": [
      "^@reduxjs/(.*)$",
      "^ojs/(.*)$",
      "^[./](.*).module",
      "^[./](.*).controller",
      "^[./](.*).service",
      "^src/(.*)$",
      "^[./]"
    ],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
  